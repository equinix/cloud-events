                    },
                    "description": "Specific details of the metric"
                },
                "message": {
                    "type": "string",
                    "description": "Supporting message describing the event"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "MetricEvent Event Data",
            "description": "The data within all MetricEvent events."
        },
